<Window x:Class="First_Class_First_Time.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:First_Class_First_Time"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="/zd_shkola1.png" />
    </Window.Background>
    <Canvas>
        <TextBox x:Name="InPut" Height="50" Width="100" Canvas.Left="136" Canvas.Top="239" ToolTip="Ввод" PreviewTextInput="InPut_PreviewTextInput" ></TextBox>
        <TextBlock x:Name="OutPut" Height="50" Width="100" Canvas.Left="536" Canvas.Top="239" HorizontalAlignment="Left" VerticalAlignment="Center" ToolTip="Вывод" Background="White"></TextBlock>
        <Button x:Name="ConvertThis" Height="50" Width="100" Click="ConvertThis_Click" Canvas.Left="336" Canvas.Top="284">Решить</Button>
    </Canvas>
</Window>
